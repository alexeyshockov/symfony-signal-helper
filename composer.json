    "homepage":    "http://github.com/alexeyshockov/symfony-signal-helper",
    "license":     "MIT",
    "support":     {
        "issues": "https://github.com/alexeyshockov/symfony-signal-helper/issues"
    },
    "authors": [
        {
            "name":     "Alexey Shokov",
            "email":    "alexey@shockov.com"
        }
    ],
    "require": {
